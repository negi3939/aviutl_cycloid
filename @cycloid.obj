@サイクロイド歯車
--color:0xffff00
--track0:基礎半径,0,1000,100,0.5
--track1:外周半径,0,1000,100,0.5
--track2:歯数,0,100,0,1
--track3:線幅,0,15,0,1
n= 5000
r = obj.getvalue(0)
iro = 0xffffff
obj.load("figure","円",color,30)
theta=0
for ii=0,n-1 do
	theta = ii*2*math.pi/n 
	x = r*math.cos(theta)
	y = r*math.sin(theta)
	obj.draw(x,y,0,0.5)
end
